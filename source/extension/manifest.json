{
  "manifest_version": 2,
  "name": "Gaurdian",
  "version": "1.0",

  "permissions": ["tabs", "activeTab", "<all_urls>"],

  "description": "Sends Information about your current running site",

  "icons": {
    "48": "logo/logo_1.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "*://*.youtube.com/*",
        "http://www.youtube.com/*"
      ],
      "js": []
    }
  ],

  "background": {
    "scripts": ["sendData.js"]
  }
}
